{
  "release": {
    "path": "./release"
  },
  "backup": {
    "path": "D:/backup/",
    "name": "{project_name}/{project_name}-{date_time}"
  },
  "structure": {
    "text": {
      "path": "text",
      "tags": [
        "text"
      ],
      "extensions": [
        "txt",
        "text"
      ]
    },
    "image": {
      "path": "image",
      "tags": [
        "image"
      ],
      "extensions": [
        "bmp",
        "bpg",
        "exif",
        "gif",
        "hdr",
        "heif",
        "jfif",
        "jpg",
        "jpeg",
        "pbm",
        "pgm",
        "png",
        "pnm",
        "ppm",
        "spiff",
        "tif",
        "tiff",
        "webp"
      ]
    },
    "presentation": {
      "path": "office/presentation",
      "tags": [
        "presentation",
        "office"
      ],
      "extensions": [
        "ppt",
        "pptx",
        "pptm",
        "potm",
        "pot",
        "ppsx",
        "ppsm",
        "pps",
        "ppam",
        "ppa",
        "potx",
        "odp",
        "sda",
        "sdd",
        "sdp",
        "uop"
      ]
    },
    "spreadsheet": {
      "path": "office/spreadsheet",
      "tags": [
        "spreadsheet",
        "office"
      ],
      "extensions": [
        "xls",
        "xlw",
        "xlt",
        "xlsx",
        "xlsm",
        "xltx",
        "xltm",
        "xlsb",
        "wk1",
        "wks",
        "sdc",
        "uos",
        "pxl",
        "wb2"
      ]
    },
    "sheet": {
      "path": "office/sheet",
      "tags": [
        "sheet",
        "office"
      ],
      "extensions": [
        "doc",
        "dot",
        "docx",
        "docm",
        "dotx",
        "dotm",
        "wpd",
        "wps",
        "uot",
        "psw"
      ]
    },
    "documents": {
      "path": "office/documents",
      "tags": [
        "documents"
      ],
      "extensions": [
        "pdf",
        "xml",
        "json"
      ]
    },
    "angular": {
      "path": "programming/node-js/angular",
      "tags": [
        "programming",
        "node-js",
        "npm",
        "web-development",
        "angular"
      ],
      "structure": {
        "files": [
          "package.json",
          "angular.json"
        ],
        "release": {
          "path": "/dist",
          "commands": [
            "ng build --prod"
          ]
        }
      }
    },
    "express": {
      "path": "programming/node-js/express",
      "tags": [
        "programming",
        "node-js",
        "npm",
        "api",
        "express"
      ],
      "structures": [
        {
          "files": [
            "index.js",
            "package.json"
          ],
          "release": {
            "path": "/"
          }
        },
        {
          "files": [
            "app.js",
            "package.json"
          ],
          "release": {
            "path": "/"
          }
        },
        {
          "files": [
            "package.json",
            "dist/index.js",
            "src/index.ts"
          ],
          "release": {
            "path": "/dist",
            "commands": "npm run build"
          }
        }
      ]
    },
    "node-js": {
      "path": "programming/node-js",
      "tags": [
        "programming",
        "node-js",
        "npm"
      ],
      "structure": {
        "files": [
          "package.json"
        ],
        "release": {
          "path": "/"
        }
      }
    },
    "python": {
      "path": "programming/python",
      "tags": [
        "programming",
        "python"
      ],
      "structure": {
        "files": [
          "*.py"
        ],
        "release": {
          "path": "/dist"
        }
      }
    },
    "html": {
      "path": "programming/html",
      "tags": [
        "programming",
        "web-development",
        "html"
      ],
      "structure": {
        "files": [
          "index.html"
        ],
        "release": {
          "path": "/"
        }
      }
    },
    "php": {
      "path": "programming/php",
      "tags": [
        "programming",
        "web-development",
        "php"
      ],
      "structure": {
        "files": [
          "index.php"
        ],
        "release": {
          "path": "/"
        }
      }
    }
  }
}